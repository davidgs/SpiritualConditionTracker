{
  "expo": {
    "name": "Spiritual Condition Tracker",
    "slug": "spiritual-condition-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#01040c"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aarecovery.tracker",
      "buildNumber": "1",
      "infoPlist": {
        "NSCalendarsUsageDescription": "This app needs access to your calendar to add meetings and set reminders.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to find nearby meetings and members.",
        "NSBluetoothAlwaysUsageDescription": "This app needs access to Bluetooth to discover nearby members.",
        "UIBackgroundModes": [
          "bluetooth-central",
          "location"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#01040c"
      },
      "package": "com.aarecovery.tracker",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "BLUETOOTH",
        "BLUETOOTH_ADMIN",
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "aa-recovery-tracker"
      }
    },
    "owner": "your-expo-username"
  }
}
