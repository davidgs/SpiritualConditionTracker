modules = ["nodejs-20", "python-3.11"]

[nix]
channel = "stable-24_05"
packages = ["jdk11", "imagemagick"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "ExpoApp"

[[workflows.workflow]]
name = "ExpoApp"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "./auto-accept-alternate-port.sh"
waitForPort = 5000

[deployment]
run = ["sh", "-c", "node deployment-server.js"]
deploymentTarget = "cloudrun"

