{
  "expo": {
    "name": "Spiritual Condition Tracker",
    "slug": "spiritual-condition-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#01040c"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.davidgs.spiritualconditiontracker",
      "buildNumber": "1",
      "infoPlist": {
        "NSCalendarsUsageDescription": "This app needs access to your calendar to add meetings and set reminders.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to find nearby meetings and members.",
        "NSBluetoothAlwaysUsageDescription": "This app needs access to Bluetooth to discover nearby members.",
        "NSBluetoothPeripheralUsageDescription": "This app uses Bluetooth to connect with nearby AA members.",
        "UIBackgroundModes": [
          "bluetooth-central",
          "bluetooth-peripheral",
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#01040c"
      },
      "package": "com.davidgs.spiritualconditiontracker",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "BLUETOOTH",
        "BLUETOOTH_ADMIN",
        "BLUETOOTH_SCAN",
        "BLUETOOTH_CONNECT",
        "BLUETOOTH_ADVERTISE",
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "publicPath": "app"
    },
    "extra": {
      "eas": {
        "projectId": "fc5da254-b364-461a-b064-b16448479301"
      }
    },
    "owner": "davidgs",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "enabled": true
    }
  }
}
